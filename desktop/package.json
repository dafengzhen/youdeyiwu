{
  "name": "youdeyiwu",
  "version": "1.0.0",
  "author": "dafengzhen",
  "description": "youdeyiwu is an open-source lightweight forum",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ng:build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev": "concurrently npm:start npm:electron --kill-others",
    "build": "npm run ng:build && npm run dist",
    "electron": "wait-on tcp:4200 && cross-env NODE_ENV=development ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "pack": "cross-env NODE_ENV=production electron-builder --dir",
    "dist": "cross-env NODE_ENV=production electron-builder -w"
  },
  "dependencies": {
    "@angular/animations": "^17.3.5",
    "@angular/common": "^17.3.5",
    "@angular/compiler": "^17.3.5",
    "@angular/core": "^17.3.5",
    "@angular/forms": "^17.3.5",
    "@angular/platform-browser": "^17.3.5",
    "@angular/platform-browser-dynamic": "^17.3.5",
    "@angular/router": "^17.3.5",
    "@tanstack/angular-query-experimental": "^5.31.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "date-fns": "^3.6.0",
    "highlight.js": "^11.9.0",
    "nanoid": "^5.0.7",
    "ngx-cookie-service": "^17.1.0",
    "photoswipe": "^5.4.3",
    "query-string": "^9.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.5",
    "@tanstack/angular-query-devtools-experimental": "^5.31.0",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5",
    "wait-on": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dafengzhen/youdeyiwu.git"
  },
  "keywords": [
    "youdeyiwu",
    "forum",
    "blog",
    "note",
    "seo"
  ],
  "bugs": {
    "url": "https://github.com/dafengzhen/youdeyiwu/issues"
  },
  "homepage": "https://github.com/dafengzhen/youdeyiwu#readme",
  "build": {
    "appId": "com.youdeyiwu.youdeyiwu",
    "directories": {
      "buildResources": "src/assets/images/icons",
      "output": "out"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
